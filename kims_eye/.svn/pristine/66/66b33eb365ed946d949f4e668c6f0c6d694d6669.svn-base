/* 게시판 */
.notice-board {table-layout: fixed; padding: 15px;
    border-spacing: 0;
    width: 750px; text-align: center; font-size: 12px;}
.notice-board th {border-top: 1px solid #000; border-bottom: 1px solid #aaa;
    border-width: 1px 0; letter-spacing: .2em; background-color: #efefef}
.notice-board th, .notice-board td {padding: 10px;}
/* .notice-board tr + td {text-align: center;} */
.notice-board td:nth-child(2) {white-space: nowrap; overflow: hidden;
    text-overflow: ellipsis; text-align: left;}
.notice-board a {text-decoration: none; color: #000;}    
.notice-board .col-no {width: 74px; text-align: center;}
.notice-board .col-title {width: 370px;}
.notice-board .col-writer {width: 100px;}
.notice-board .col-hits {width: 80px;}
/* 페이지바 */
.page_wrap {text-align:center;}
.page_nation {display:inline-block; margin-top: 25px;}
.page_nation a {display:block; margin:0 3px; float:left; border:1px solid #e6e6e6;
	width:28px; height:28px; line-height:2.3em; text-align:center; background-color:#fff;
	font-size:13px; color:#999999; text-decoration:none;}
.page_nation .arrow {border:1px solid #ccc;}
.page_nation .pprev {background:#f8f8f8 url('../project_images/page_pprev.png') no-repeat center center;
	margin-left:0;}
.page_nation .prev {background:#f8f8f8 url('../project_images/page_prev.png') no-repeat center center;
	margin-right:7px;}
.page_nation .next {background:#f8f8f8 url('../project_images/page_next.png') no-repeat center center;
	margin-left:7px;}
.page_nation .nnext {
	background:#f8f8f8 url('../project_images/page_nnext.png') no-repeat center center;
	margin-right:0;}
.page_nation a.active {
	background-color: #003963; color:#fff; border:1px solid #003963;}
/* 글열람 */
.reading-content {width: 750px; float: left;}
.reading {width: 750px; table-layout:fixed; float: left; border-collapse: collapse;
    margin-top: 20px; margin-bottom: 35px;}
.reading td {border-top: 1px solid #efefef; border-bottom: 1px solid #efefef;
    padding: 12px; width: 50px;}
.reding td + td {text-align: left;}
.col-reading1 {width: 12px; background-color: #efefef; text-align: center;}
.col-reading2 {text-align: left;}
.reading-content span, .reading-reply span {width: 750px; line-height: 1.8em;}
.reading-reply {color: #777; margin-top: 30px;}
/* .do-prev {width: 97px; height: 57px; margin: 0 auto;} */
.do-prev {width: 97px; margin: 0 auto;}
/* 첨부파일 */
.dl {text-decoration: none; color: #000;}
.dl:hover {color: #0693f8;}
/* .list-btn {width: 750px; margin: 0 auto;} */
.list-btn {width: 97px; background-color:#003963; border-style: none;
    border-radius: 10px; margin: 30px auto; padding: 17px; font-size: 1.2em;
    color: #fff; display: block;}
/* .do-prev, .list {width: 97px; background-color:#003963; border-style: none;
    border-radius: 10px; margin: 30px auto; padding: 17px; font-size: 1.2em;
    color: #fff; display: block;} */
.list-btn:hover, .list-btn:active, .list-btn:focus {background-color:#20425a}
.small-btn input {width: 80px; padding: 15px; margin-left: 12px; margin-bottom: 30px; border-style: none; float: right;
     border-radius: 8px;}
/* 댓글 */
.reply-header {margin-top: 28px; margin-bottom: 13px;}
.reply-header button {margin: 13px 0; padding: 7px; float: right; border-style: none; border-radius: 8px; background: #efefef;
	color: #777;}
.reply-count {margin: 13px 0; padding: 10px; color: #777; float: left; border-bottom: 1px solid #0693f8;}
.reply-content {clear: both; position: relative; padding: 15px; border-bottom: 1px solid #ccc;}
.reply-content-header {color: #777; margin-bottom: 13px;}
.reply-content > .reply-body {width: 720px; min-height: 50px;}
.reply-content .reply-btns {position: absolute; top: 15px; right: 0px;}
.reply-btns button {padding: 5px; border-style: none; background: #fff; color:#777; cursor: pointer;}
.reply-btns button:hover, .reply-btns button:active, .reply-btns button:focus {color: #0693f8;}
.reply-form {text-align: right;}
.reply-form button {margin-bottom:13px; padding: 7px; border-style: none; border-radius: 8px; background: #efefef;}
.reply-form textarea {width: 100%; margin: 20px 0;}


.reply-modify {display: none;}
/* 글수정 */
.modify-area {width: 750px; float: left; overflow: auto;}
.modify-top, .reservation-form {width: 750px; table-layout: fixed; padding: 12px; border-collapse: collapse;}
.modify-top .title {width: 546px; padding: 8px;}
.modify-top select {width: 100px; padding: 10px;}
.modify-top .email {width: 230px; padding: 8px;}
.modify-top label {margin: 0%;}
.modify-top input[type="email"] {width:230px; padding: 8px;}
.modify-top input[type="checkbox"] {margin-right: 15px;}
.modify-top td, .reservation-form td {padding: 10px; text-align: center;}
.modify-top td + td, .reservation-form td + td {text-align: left;}
.modify-col1 {background-color: #efefef; width: 37px; font-size: 12px;}
.modify-content {margin: 20px 0;}
.modify-content textarea {width: 700px; height: 250px; border-style: none;
    border: 1px solid #dcdcdc; overflow: auto; line-height: 1.8em;
    margin-top: 25px; margin-bottom: 15px;}
.modify-area input[type="file"] {width: 240px; margin-bottom: 20px;}
.modify-btn-wrap {width: 750px; margin: 0 auto;}
.modify-btn {width: 201px; margin: 0 auto; margin-bottom: 40px;}
.modify-btn .modify {width: 97px; padding: 17px; margin-left: 10x; border-style: none; border-radius: 10px; background-color: #003963; color: #fff;} 
#cancel {width: 97px; padding: 17px; border-style: none; border-radius: 10px; background-color: #dcdcdc; color: #fff;}
.modify-btn .cancel:hover {background-color: #ccc;}
.list-btn .modify {margin: 0;}
/* 에디터 */
.ck-editor__editable {min-height: 250px;}
/* 검색바 */
.search {width: 548px; margin: 30px auto; clear: both;}
.search select {width: 110px; padding: 10px;}
.search input {width: 300px; padding: 10px;}
/* .search button {width: 70px; padding: 10px; border-style: none; background-color: #003963; color: #fff;} */
.search input[type="submit"] {width: 70px; padding: 10px; border-style: none; background-color: #003963; color: #fff;}

.write-btn {float: right; width: 81px; padding: 15px; border-style: none; border-radius: 8px; background-color: #efefef;}
.fa-search {color:#0693f8}
.fa-lock, .fa-images {color: #aaa; margin-left: 7px;}